<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tpairviafx.OfficeManagerController">
   <children>
      <TextField fx:id="amountTextField" layoutX="283.0" layoutY="98.0" prefHeight="26.0" prefWidth="35.0" />
      <TableView fx:id="staffTableView" layoutX="14.0" layoutY="69.0" prefHeight="131.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="staffIDColumn" prefWidth="75.0" text="Staff ID" />
        </columns>
      </TableView>
      <Button fx:id="assignButton" layoutX="272.0" layoutY="142.0" mnemonicParsing="false" onAction="#assignBlanks" text="Assign" />
      <TextField fx:id="searchTextField" layoutX="14.0" layoutY="35.0" promptText="Search Staff by name" />
      <ChoiceBox fx:id="blankChoiceBox" layoutX="232.0" layoutY="56.0" prefWidth="150.0" />
      <Button fx:id="logoutButton" layoutX="496.0" layoutY="33.0" mnemonicParsing="false" onAction="#logOut" text="Log Out" />
   </children>
</AnchorPane>
